<div class = "center jumbotron">
  <h1> Select a country </h1>

<div class="list">
  <table>
    <tbody>
      <% @countries.each do |country| %>   
        <tr>
          <td> 
              <li><p><%= link_to country.name, country_path(country) %> </p> </li>
          </td>
          <td>
            <img src = <%= country.image%> style="width:100px">
          </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>