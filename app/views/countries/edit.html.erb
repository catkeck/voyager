<%= form_for @country do |form| %>
    <%= form.label :name %> <br />
    <%= form.text_field :name %> <br />
    <%= form.label :continent %> <br />
    <%= form.text_area :continent %><br />
    <%= form.label :image %> <br />
    <%= form.text_field :image %> <br />
    <%= form.submit "Edit country" %><br />
  <% end %>